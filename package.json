{
  "name": "netflix-fullstack",
  "version": "1.0.0",
  "type": "module",
  "description": "Netflix clone with React and Express; frontend built with Karl Hadwen's guidance; backend my own",
  "main": "index.js",
  "scripts": {
    "api": "nodemon -r dotenv/config api",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run frontend\" \"npm run api\"",
    "start": "NODE_ENV=production node -r dotenv/config api",
    "data:import": "node -r dotenv/config api/_data/seeder.js",
    "data:destroy": "node -r dotenv/config api/_data/seeder.js -d"
  },
  "author": "Andrew Sanchez",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "colors": "^1.4.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.16",
    "morgan": "^1.10.0"
  }
}
